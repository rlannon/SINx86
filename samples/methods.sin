// methods.sin
// a demo of struct methods

decl void print(decl string s &final);

def struct myStruct {
    alloc int _size;
    alloc string _name;

    def string get_name() {
        return this._name;
    }
}

def int main(alloc array<string> args &dynamic) {
    alloc string s;
    let s = "hello";
    
    alloc myStruct m;
    let m._size = 0;
    let m._name = "MYSTRUCT M";
    @print( @m.get_name() );

    return 0;
}
